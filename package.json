{
  "name": "gthubweb",
  "version": "0.1.0",
  "description": "New GTHub's website design using Node.js, Express and Nuxt.js",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server --watch api",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "yarn run lint && NODE_ENV=test mocha -R spec api/test --exit",
    "test_connection": "node ./database/test_connection.js",
    "test_api": "cross-env NODE_ENV=test mocha -R spec api/test --exit",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "yarn run lint:js && yarn run lint:style",
    "create_database": "yarn run create_tables && yarn run init_data && yarn run insert_testdata",
    "create_testdatabase": "cross-env NODE_ENV=test yarn run create_tables && cross-env NODE_ENV=test yarn run init_test",
    "create_tables": "node ./database/create_tables.js",
    "init_data": "node ./database/init.js",
    "init_test": "node ./database/init_test.js",
    "insert_testdata": "node ./database/insert_testdata.js",
    "tailwind-config-viewer": "tailwind-config-viewer -o -p 3006"
  },
  "dependencies": {
    "@nuxt/types": "^2.14.7",
    "@nuxtjs/auth": "^4.9.1",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/dayjs": "^1.3.0",
    "@types/chai": "^4.2.14",
    "@types/express": "^4.17.9",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.10",
    "async": "^3.2.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.5",
    "country-code-emoji": "^2.2.0",
    "country-list": "^2.2.0",
    "cross-env": "^7.0.2",
    "dayjs": "^1.10.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "ical-generator": "^2.0.0",
    "jsonwebtoken": "^9.0.0",
    "mariadb": "^2.5.1",
    "method-override": "^3.0.0",
    "nuxt": "^2.15.3",
    "primeflex": "^3.0.0",
    "primeicons": "^4.1.0",
    "primevue": "^2.3.0",
    "quill": "^1.3.7",
    "sequelize": "^6.5.0",
    "supertest": "^6.0.1",
    "vue": "^2.6.12",
    "vue2-timepicker": "^1.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/eslint-parser": "^7.12.13",
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.1",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/tailwindcss": "^4.1.2",
    "@types/mocha": "^9.0.0",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "css-loader": "^5.0.0",
    "eslint": "^7.19.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.5.0",
    "fibers": "^5.0.0",
    "mocha": "^9.0.0",
    "nodemon": "^2.0.6",
    "postcss": "^8.3.0",
    "sass": "^1.32.6",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.9.0",
    "stylelint-config-concentric-order": "^4.0.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.2.3",
    "webpack": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gthubdev/website.git"

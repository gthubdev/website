{
  "name": "gthubweb",
  "version": "0.1.0",
  "description": "New GTHub's website design using Node.js and Express",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server --watch api",
    "test_connection": "node ./database/test_connection.js",
    "test": "npm run lint && cross-env NODE_ENV=test mocha -R spec api/test --exit",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "create_database": "npm run create_tables && npm run init_data && npm run insert_testdata",
    "create_tables": "node ./database/create_tables.js",
    "init_data": "node ./database/init.js",
    "insert_testdata": "node ./database/insert_testdata.js"
  },
  "dependencies": {
    "@nuxtjs/auth": "^4.8.4",
    "@nuxtjs/axios": "^5.9.0",
    "@types/async": "^3.0.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/country-list": "^2.1.0",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/moment-timezone": "^0.5.12",
    "@types/supertest": "^2.0.8",
    "async": "^3.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "country-code-emoji": "^1.0.0",
    "country-list": "^2.2.0",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "eslint-plugin-html": "^6.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "ical-generator": "^1.9.2",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^2.1.4",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "nuxt": "^2.11.0",
    "nuxt-vue-material": "^1.2.0",
    "sequelize": "^5.21.3",
    "supertest": "^4.0.2",
    "vue": "^2.6.11",
    "vue-ctk-date-time-picker": "^2.4.0",
    "vue-material": "^1.0.0-beta-11",
    "vue-server-renderer": "^2.6.11",
    "vuejs-paginate": "^2.1.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@types/chai": "^4.2.7",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-nuxt": "^0.5.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "mocha": "^6.2.2",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.0"
  },
